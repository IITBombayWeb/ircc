{#
/**
 * @file
 * Default theme implementation of a views exposed form.
 *
 * Available variables:
 * - form: A render element representing the form.
 *
 * @ingroup templates
 *
 * @see template_preprocess_views_exposed_form()
 */
#}
{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
  {{ q }}
{% endif %}
<div class="form--inline form-inline clearfix">
  {# {{ kint(form) }}  #}
  <div class="row">
    <div class="col-md-6">
      {{ form.interview_date_start }}
    </div>

    <div class="col-md-6">
      {{ form.interview_date_end }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      {{ form.appointed }}
    </div>

    <div class="col-md-6">
      {{ form|without('interview_date_start', 'interview_date_end', 'appointed') }}
    </div>
  </div>
</div>
